{
  "expo": {
    "name": "inventory_app",
    "slug": "inventory_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "caf5580c-ffc9-4eb2-ae71-1388b95c3437"
      }
    },
    "owner": "linhld9",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/caf5580c-ffc9-4eb2-ae71-1388b95c3437",
      "requestHeaders": {
        "expo-channel-name": "main"
      }
    }
  }
}
